{
  "version": 2,
  "name": "blind-type-backend",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "buildCommand": "nest build",
  "devCommand": "nest start --watch",
  "installCommand": "yarn",
  "outputDirectory": "dist"
}
